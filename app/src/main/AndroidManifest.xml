<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kelompoksatu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KelompokSatu">
        <activity android:name=".pesanan.ui.DetailPesananActivity"></activity>
        <activity android:name=".guide.ui.EditGuideActivity" />
        <activity android:name=".guide.ui.DetailGuideActivity" />
        <activity android:name=".guide.ui.TambahGuideActivity" />
        <activity android:name=".paket.ui.EditPaketActivity" />
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".paket.ui.TambahPaketActivity" />
        <activity android:name=".paket.ui.DetailPaketActivity" />
        <activity android:name=".ui.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.MainActivity" />
    </application>

</manifest>